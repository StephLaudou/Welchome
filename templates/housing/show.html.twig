    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ housing.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ housing.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ housing.description }}</td>
            </tr>
            <tr>
                <th>MaxTraveler</th>
